 "html": "<!DOCTYPE html>\n<html>\n<head>\n    <title>Field Service Report</title>\n    <style>\n       \n        table {\n            width: 100%;\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid black;\n            padding: 5px;\n            text-align: center;\n        }\n        th.full-row {\n            width: 100%;\n        }\n        td.half-row {\n            width: 50%;\n        }\n        td.third-row {\n            width: 33.33%;\n        }\n        td.equipment-attended {\n            text-align: left; /* Center align the entire td */\n        }\n        td.equipment-attended sp {\n            display: inline-block;\n            text-align: center; /* Left align text inside the span */\n        }\n        .page-break {\n            page-break-before: always;\n        }\n    </style>\n</head>\n<body>\n    <table>\n        <tr style=\"width:100%\">\n            <td class=\"full-row\" colspan=\"5\" style=\"font-size:18px\">Field Service Report</td>\n        </tr>\n        <tr style=\"height:80px\">\n            <td class=\"\" style=\"text-align:left\" colspan=\"2\">Dealer : {{doc.dealer}}</td>\n            <td class=\"equipment-attended\" colspan=\"3\"><sp>Equipment Attended</sp><br>Split AC  : {{doc.model_name}}<br>Packaged AC</td>\n        </tr>\n        <tr style=\"height:100px;\">\n            <td class=\"\" colspan=\"2\" style=\"text-align:left\">Customer Name : {{doc.customer_name}}<br>Address:{{doc.customer_address}}<br>Mobile no: {{doc.customer_phone_number}}<br>Pincode:{{doc.customer_pincode}}</td>\n            <td class=\"third-row\" colspan=\"3\" id=\"model_details\">\n                <table style=\"\">\n                    {% for row in doc.table_fbbp%}\n                    {% if row.outdoor==1 %}\n                        <tr>\n                            <td style=\"border-bottom:0px! important ; padding: px;font-size:14px\">ODU Model No</td>\n                            <td style=\"border: ; padding: px;font-size:10px\">{{row.model_name}}</td> <!-- Empty column 1 -->\n                        </tr>\n                        <tr>\n                            <td style=\"border:; padding: px;font-size:14px\">ODU Serial No</td>\n                            <td style=\"border:; padding: px;font-size:10px\">{{row.serial_number}}</td> <!-- Empty column 1 -->\n                        </tr>\n                    \n                    {% else %}\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:14px\">IDU Model No</td>\n                        <td style=\"border:; padding: px;font-size:10px\">{{row.model_name}}</td> <!-- Empty column 1 -->\n                    </tr>\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:14px\">IDU Serial No</td>\n                        <td style=\"border:; padding: px;width:30%;font-size:10px;\">{{row.serial_number}}</td> <!-- Empty column 1 -->\n                    </tr>\n                    {% endif %}\n                    {% endfor %}\n                </table>  \n            </td>\n        </tr>\n        <tr >\n            <td style=\"width:50%;text-align:left\" colspan=\"2\">Name of the Complaint : {{doc.name_of_complaint}}</td>\n            <td style=\"width:30%;text-align:left\">Call Details</td>\n            <td style=\"width:10%;text-align:left\">Date</td>\n            <td style=\"width:10%;text-align:left\">Time</td>\n        </tr>\n        <tr style=\"\">\n            <td style=\"text-align:left;width:50%\" colspan=\"2\">\n                Action Taken<br>{{doc.action_taken}}<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>Call Attended by  :{{doc.call_attended_by_name}}\n            </td>\n           \n            <td style=\"width:50%\" colspan=\"3\">\n                <table style=\"width:100%\">\n                    <tr>\n                        <td style=\"border: ; padding: px;font-size:10px;text-align:left\">Call Attended On</td>\n                        <td style=\"border: ; padding: px;font-size:10px\">{{doc.call_attended_date}}</td> <!-- Empty column 1 -->\n                        <td style=\"border: ; padding: px;font-size:10px\">{{doc.call_attended_time}}</td> <!-- Empty column 2 -->\n                    </tr>\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:10px;text-align:left\">Work Completed on</td>\n                        <td style=\"border:; padding: px;font-size:10px;\">{{doc.work_completed_date}}</td> <!-- Empty column 1 -->\n                        <td style=\"border:; padding: px;font-size:10px;\">{{doc.work_completed_time}}</td> <!-- Empty column 2 -->\n                    </tr>\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:10px;text-align:left\"></td>\n                        <td style=\"border:; padding: px;\">Unit 1</td> <!-- Empty column 1 -->\n                        <td style=\"border:; padding: px;\">Unit 2</td> <!-- Empty column 2 -->\n                    </tr>\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:10px;text-align:left\">Test Measurement</td>\n                        <td style=\"border:; padding: px;width:30%\"></td> <!-- Empty column 1 -->\n                        <td style=\"border:; padding: px;width:30%\"></td> <!-- Empty column 2 -->\n                    </tr>\n                    \n                    <tr>\n                        <td style=\"border: ; padding: px;font-size:10px;text-align:left\">Voltage</td>\n                        <td style=\"border: ; padding: px;\"></td> <!-- Empty column 1 -->\n                        <td style=\"border: ; padding: px;\"></td> <!-- Empty column 2 -->\n                    </tr>\n                    <tr>\n                        <td style=\"border:; padding: px;font-size:10px;text-align:left\">Total Amps</td>\n                        <td style=\"borde